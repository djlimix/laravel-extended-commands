{
  "name": "djlimix/laravel-extended-commands",
  "description": "A simple package to add more Make: commands to Laravel.",
  "type": "library",
  "keywords": ["laravel", "extended", "commands", "artisan", "make", "interface", "service"],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^8.0|^9.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Djlimix\\ExtendedCommands\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Maximili√°n Csank",
      "email": "info@limixmedia.com"
    }
  ],
  "minimum-stability": "dev",
  "require-dev": {
    "laravel/pint": "dev-main",
    "phpstan/phpstan": "1.9.x-dev"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Djlimix\\ExtendedCommands\\Providers\\PackageServiceProvider"
      ]
    }
  }
}
